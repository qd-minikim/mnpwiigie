          <image style='width:60px;height:60px' mode='scaleToFill' src='{{item.theme_img_url}}'></image>
        </view>
        <view class='helps-view-1-1-2 '>
          <view class='font-size-4'>{{item.merchant_brand_name_des}}</view>

