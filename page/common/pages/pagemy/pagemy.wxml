<!-- 申请记录 -->
<template name="servlist_1">
  <block wx:for="{{servlistArray_1}}" wx:key="" wx:for-index="inde" wx:for-item="item">
    <block wx:if="{{inde>0}}">
      <view class="gap-line" style="flex-direction:column;"></view>
    </block>
    <view class='customer-view'>
      <view class='customer-view-1'>
        <view class='font-size-7' style='flex:1;text-align:left'>服务单号：{{item.id}}</view>
        <view class='font-size-7'>{{item.service_type_name}}</view>
      </view>
      <view class="gap-line" style="flex-direction:column;"></view>
      <view class='customer-view-2 '>
        <view class='align-center'>
          <image class='customer-view-2-1-image' src='{{item.image_url}}'></image>
        </view>
        <view class='customer-view-2-2' style='position:relative'>
          <view class='view-ellipsis-1 font-size-4' style='text-align:left'>{{item.title}}</view>

          <view style='flex:1'></view>

          <block wx:if="{{item.service_status =='51'}}">
            <view style='margin:2rpx 5rpx;position:absolute;bottom:0rpx;right:0rpx'>
              <button class='font-size-7 align-center' style='padding:0 20rpx;line-height:2.2;' catchtap='suhjrereceve' data-serviceid='{{item.id}}' data-username='{{item.username}}' data-buyuserid='{{item.buy_userid}}'>
                <image style='width:30rpx;height:30rpx;' src='/image/figure_b_101.png'></image>
                <view style='margin:0 5rpx;'>确认收货</view>
              </button>
            </view>
          </block>
        </view>
      </view>
      <view class="gap-line" style="flex-direction:column;"></view>
      <view catchtap='servdetaPage' data-serviceid='{{item.id}}' class='customer-view-3 align-center'>
        <view class='align-center'>
          <view style='padding:2rpx 8rpx;margin:2rpx 10rpx;border:1rpx dashed #ccc' class='font-size-6'>{{item.service_status_name}}</view>
        </view>
        <view class='align-center' style='background:#ccc;width:5rpx;height:45rpx;'></view>
        <view class='font-size-6' style='flex:1;text-align:left;margin:2rpx 10rpx;'>{{item.operate_comment}}</view>
        <view>
          <image style='width:30rpx;height:30rpx' src='/image/arrow_right_1.png'></image>
        </view>
      </view>
      <view class="gap-line" style="flex-direction:column;"></view>
    </view>
  </block>
  <block wx:if="{{servlistsearched_1 && servlistArray_1.length==0}}">
    <view class='ordergift-view font-size-6'>
      还没有数据
    </view>
  </block>
</template>


<!-- 我的收藏 -->


<template name="keeps">

  <block wx:for="{{keepsInfo}}" wx:key="" wx:for-index="inde" wx:for-item="item">

    <block wx:if="{{inde>0}}">
      <view class="gap-line" style="flex-direction:column;"></view>
    </block>
    <view class='keeps-view-1' catchtap='showDetail' data-upmarkid='{{item.upmarkid}}' data-requir='{{item.requirementid}}'>
      <view class='keeps-view-1-1'>
        <view class='keeps-view-1-1-1 '>
          <image style='width:60px;height:60px' mode='scaleToFill' src='{{item.theme_img_url}}'></image>
        </view>
        <view class='keeps-view-1-1-2 '>
          <view class='font-size-4'>{{item.merchant_brand_name_des}}</view>

          <block wx:if="{{item.deal_type =='0'}}">
            <view class='font-size-6 font-color-7'>{{item.deal_type_name}}</view>


          </block>
          <block wx:else>
            <view class='font-size-6 font-color-4'>门市价：￥{{item.list_price}}</view>

          </block>
          <block wx:if="{{item.deal_type !='0'}}">
            <view class='font-size-6 font-color-7'>{{item.link_price_name}}:{{item.link_discount}}</view>
          </block>
        </view>
      </view>
      <view class='view-ellipsis-1  font-size-4'>{{item.title}}</view>
      <view style='flex-direction:row;display:flex; '>
        <view class='font-size-6 font-color-4' style='width:5em'>驻足好友：</view>
        <view class='font-size-6 font-color-4' style='flex-direction:row;display:flex;flex:1'>
          <block wx:for="{{item.my_friends}}" wx:key="" wx:for-index="indexf" wx:for-item="itemf">

            <image class="keeps-view-1-1-1-f-image" src='{{itemf.friend_image_url}}'></image>
          </block>
          ({{item.my_friends_num}}人)
        </view>
      </view>
      <view class='keeps-view-1-4 '>
        <view class='font-size-6 font-color-4' style='flex:1'>活动截止：{{item.deadlinetime}}</view>
        <view class='align-right' style='flex-direction:row;display:flex;'>
          <view class='font-size-7 font-color-4' style='width:100rpx'>
            <image class='keeps-view-1-4-figure' src='/image/figure_29.png'></image>{{item.zfnum}}
          </view>
          <view class='font-size-7 font-color-4' style='width:100rpx'>
            <image class='keeps-view-1-4-figure' src='/image/figure_31.png'></image>{{item.evaluate_num}}
          </view>
          <view class='font-size-7 font-color-4' style='width:100rpx'>
            <image class='keeps-view-1-4-figure' src='/image/figure_30.png'></image>{{item.all_copies}}
          </view>
        </view>
      </view>

    </view>
  </block>
</template>



<!-- 我的足迹 -->


